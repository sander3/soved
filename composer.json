{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "doctrine/dbal": "^2.7",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "graham-campbell/github": "^8.2",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.0",
        "laravel/nova": "~3.0",
        "laravel/telescope": "^3.2",
        "laravel/tinker": "^2.0",
        "mazedlx/laravel-feature-policy": "dev-master",
        "php-http/guzzle6-adapter": "^2.0",
        "predis/predis": "^1.1",
        "soved/laravel-helpers": "^1.0",
        "spatie/browsershot": "^3.35",
        "spatie/laravel-csp": "^2.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-translatable": "^4.1",
        "spatie/nova-translatable": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sander3/laravel-feature-policy"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "@php artisan telescope:publish",
            "@php artisan horizon:publish"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
